# n8n kube play environment (sample)

# Image to run locally (built via Podman)
N8N_IMAGE=localhost/n8n:latest

# n8n platform settings
N8N_HOST=0.0.0.0
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/

# Security (use a strong random value in real usage)
N8N_ENCRYPTION_KEY=please-change-me

# Postgres connection
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8npassword
